{
	"compilerOptions": {
		"target": "ESNext",
		"module": "ESNext",
		"moduleResolution": "node",
		"esModuleInterop": true,
		"lib": ["ESNext"],
		"types": ["@cloudflare/workers-types"],
		"jsx": "react-jsx",
		"jsxImportSource": "hono/jsx",
		"paths": {
			"@/*": ["./src/*"]
		},
		"forceConsistentCasingInFileNames": true,
		"noUnusedLocals": true,
		"noUnusedParameters": true,
		"skipLibCheck": true,
		"strict": true,
		"strictNullChecks": true,
		"allowJs": false,
		"baseUrl": "."
	}
}
